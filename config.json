{
  "$schema": "https://raw.githubusercontent.com/bai/dev/refs/heads/main/config.schema.json",
  "configUrl": "https://gist.githubusercontent.com/bai/d5a4a92350e67af8aba1b9db33d5f077/raw/config.json",
  "defaultOrg": "acmesoftware",
  "logLevel": "debug",
  "defaultProvider": "github",
  "baseSearchPath": "~/src",
  "orgToProvider": {
    "acmesoftware": "gitlab"
  },
  "telemetry": {
    "mode": "disabled"
  },
  "miseGlobalConfig": {
    "tools": {
      "sops": "latest",
      "age": "latest",
      "bun": "latest",
      "rust": "latest",
      "aws-cli": "latest",
      "fd": "latest",
      "fzf": "latest",
      "gcloud": "latest",
      "go": "latest",
      "golangci-lint": "latest",
      "jq": "latest",
      "node": ["latest", "lts"],
      "python": ["3.12", "latest"],
      "ruby": "latest",
      "terraform": "latest",
      "terragrunt": "latest",
      "uv": "latest"
    },
    "settings": {
      "trusted_config_paths": [
        "~/.dev",
        "~/src/gitlab.com/bai",
        "~/src/gitlab.com/acmesoftware",
        "~/src/github.com/acmesoftware",
        "~/src/github.com/AcmeSoftware"
      ],
      "experimental": true,
      "idiomatic_version_file_enable_tools": []
    }
  },
  "miseRepoConfig": {
    "tools": {
      "python": ["3.12", "latest"]
    }
  }
}
