{
  "configUrl": "https://raw.githubusercontent.com/bai/dev/main/docs/examples/configs/bai.json",
  "defaultOrg": "flywheelsoftware",
  "orgToProvider": {
    "flywheelsoftware": "gitlab"
  },
  "miseGlobalConfig": {
    "tools": {
      "bun": "latest",
      "rust": "latest",

      "sops": "latest",
      "age": "latest",

      "aws-cli": "latest",
      "fd": "latest",
      "fzf": "latest",
      "gcloud": "latest",
      "go": "latest",
      "golangci-lint": "latest",
      "jq": "latest",
      "node": ["lts", "latest"],
      "python": ["3.12", "latest"],
      "ruby": "latest",
      "terraform": "latest",
      "terragrunt": "latest",
      "uv": "latest",

      "npm:@anthropic-ai/claude-code": "latest",
      "npm:eslint": "latest",
      "npm:npm-check-updates": "latest",
      "npm:pnpm": "latest",
      "npm:prettier": "latest",
      "npm:typescript": "latest"
    },
    "settings": {
      "trusted_config_paths": [
        "~/src/gitlab.com/flywheelsoftware",
        "~/src/github.com/Flywheel-Technologies"
      ]
    }
  },
  "miseRepoConfig": {
    "tools": {
      "python": ["3.12", "latest"]
    }
  }
}
