{
  "name": "dev",
  "version": "0.0.1",
  "private": true,
  "description": "A CLI tool for quick directory navigation and environment management",
  "type": "module",
  "module": "index.ts",
  "scripts": {
    "build": "bun build --compile --minify --sourcemap --bytecode src/index.ts --outfile dist/dev",
    "typecheck": "bun tsc --noEmit",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "format": "oxfmt --check",
    "format:fix": "oxfmt",
    "test": "bun --bun vitest --bail=1 --no-watch",
    "test:bench": "bun --bun vitest bench --bail=1 --no-watch",
    "db:generate": "drizzle-kit generate",
    "schema:generate": "bun scripts/generate-schema.ts config.schema.json"
  },
  "dependencies": {
    "@effect/cli": "0.73.2",
    "@effect/cluster": "0.56.3",
    "@effect/opentelemetry": "0.61.0",
    "@effect/platform-bun": "0.87.1",
    "@iarna/toml": "2.2.5",
    "@opentelemetry/exporter-trace-otlp-http": "0.211.0",
    "@opentelemetry/resources": "2.5.0",
    "@opentelemetry/sdk-metrics": "2.5.0",
    "@opentelemetry/sdk-trace-base": "2.5.0",
    "@opentelemetry/sdk-trace-node": "2.5.0",
    "@opentelemetry/sdk-trace-web": "2.5.0",
    "@opentelemetry/semantic-conventions": "1.39.0",
    "drizzle-orm": "0.45.1",
    "effect": "3.19.16",
    "yaml": "2.8.2",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@effect/vitest": "0.27.0",
    "@types/bun": "1.3.8",
    "drizzle-kit": "0.31.8",
    "oxfmt": "0.28.0",
    "oxlint": "1.43.0",
    "oxlint-tsgolint": "0.11.5",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "vitest": "4.0.18"
  },
  "engines": {
    "bun": ">= 1.3.0"
  }
}
